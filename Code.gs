    var —Å—É–º–º–∞ = —Å—É–º–º—ã–ü–æ–ö–∞—Ç–µ–≥–æ—Ä–∏—è–º[–∫–∞—Ç–µ–≥–æ—Ä–∏—è];
    —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã.push([–∫–∞—Ç–µ–≥–æ—Ä–∏—è, —Å—É–º–º–∞]);
  }
  
  // –ó–∞–ø–∏—Å—ã–≤–∞–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã
  var range = aiSheet.getRange(—Ä–µ–∑—É–ª—å—Ç–∞—ÇStartRow, —Ä–µ–∑—É–ª—å—Ç–∞—ÇStartCol, —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã.length, 2);
  range.setValues(—Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã);
  
  // –§–æ—Ä–º–∞—Ç–∏—Ä—É–µ–º –∫–æ–ª–æ–Ω–∫—É —Å —Å—É–º–º–∞–º–∏
  var —Å—É–º–º–∞Col = —Ä–µ–∑—É–ª—å—Ç–∞—ÇStartCol + 1;
  var —Å—É–º–º–∞Range = aiSheet.getRange(—Ä–µ–∑—É–ª—å—Ç–∞—ÇStartRow, —Å—É–º–º–∞Col, —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã.length, 1);
  —Å—É–º–º–∞Range.setNumberFormat('#,##0.00" RUB"');
  
  // –î–æ–±–∞–≤–ª—è–µ–º –∑–∞–≥–æ–ª–æ–≤–∫–∏, –µ—Å–ª–∏ –∏—Ö –Ω–µ—Ç
  if (—Ä–µ–∑—É–ª—å—Ç–∞—ÇStartRow > 1) {
    var –∑–∞–≥–æ–ª–æ–≤–æ–∫Row = —Ä–µ–∑—É–ª—å—Ç–∞—ÇStartRow - 1;
    aiSheet.getRange(–∑–∞–≥–æ–ª–æ–≤–æ–∫Row, —Ä–µ–∑—É–ª—å—Ç–∞—ÇStartCol).setValue('–ö–∞—Ç–µ–≥–æ—Ä–∏—è');
    aiSheet.getRange(–∑–∞–≥–æ–ª–æ–≤–æ–∫Row, —Å—É–º–º–∞Col).setValue('–°—É–º–º–∞');
  }
  
  SpreadsheetApp.getUi().alert('–°—É–º–º–∏—Ä–æ–≤–∞–Ω–∏–µ –∑–∞–≤–µ—Ä—à–µ–Ω–æ! –û–±—Ä–∞–±–æ—Ç–∞–Ω–æ ' + –∫–∞—Ç–µ–≥–æ—Ä–∏–∏.length + ' –∫–∞—Ç–µ–≥–æ—Ä–∏–π.');
}

